           "|>"}]}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"globalAbsError", "=", 
       RowBox[{"Norm", "[", 
        RowBox[{
         RowBox[{"globalTrans", "-", "globalApprox"}], ",", 
         "\"\<Frobenius\>\""}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"<|", 
        RowBox[{
         RowBox[{"\"\<circuit\>\"", "->", "c"}], ",", 
         RowBox[{"\"\<vertices\>\"", "->", "verts"}], ",", 
