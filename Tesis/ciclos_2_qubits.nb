              RowBox[{"optim", "[", 
               RowBox[{"[", "1", "]"}], "]"}], "/", 
              RowBox[{"Norm", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"c", "[", "\"\<GateMatrixRepresentations\>\"", "]"}],
